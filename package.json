{
  "name": "shokujin-wiki",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @repo/www dev",
    "build": "pnpm --filter @repo/www build",
    "start": "pnpm --filter @repo/www start",
    "lint": "pnpm --recursive lint",
    "lint:fix": "pnpm --recursive lint:fix",
    "check-types": "pnpm --recursive check-types",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "supabase": "^2.34.3",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.14.0"
}
